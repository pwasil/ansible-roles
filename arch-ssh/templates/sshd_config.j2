
# Generated by Ansible template

Port {{ ssh_listen_port }}
AddressFamily {{ ssh_address_family }}
ListenAddress {{ ssh_listen_address }}
Compression {{ ssh_compression }}
Ciphers {{ ssh_allowed_ciphers | join(',') }}
MACs {{ ssh_allowed_macs | join(',') }}
LoginGraceTime 2m
PermitRootLogin no
MaxAuthTries 2
MaxSessions 10
AuthorizedKeysFile .ssh/authorized_keys
UsePAM yes
PrintMotd no
X11Forwarding no
Subsystem sftp /usr/lib/ssh/sftp-server
